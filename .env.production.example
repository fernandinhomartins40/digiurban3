# =====================================================
# CONFIGURAÇÃO DE PRODUÇÃO - DIGIURBAN2
# =====================================================
# COPIE este arquivo para .env.production e configure os valores reais
# NUNCA commite .env.production no Git

# Supabase Configuration
VITE_SUPABASE_URL=https://apidigiruban.com.br
NEXT_PUBLIC_SUPABASE_URL=https://apidigiruban.com.br
VITE_SUPABASE_ANON_KEY=your_anon_key_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here

# CRÍTICO: Service Role Key (necessária para operações admin)
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Database Configuration
DATABASE_URL=postgresql://user:password@host:port/database
DB_HOST=your_db_host
DB_PORT=5432
DB_NAME=postgres
DB_USER=postgres
DB_PASS=your_db_password

# Dashboard Access
DASHBOARD_USERNAME=admin
DASHBOARD_PASSWORD=your_secure_password

# CORS Configuration
ALLOWED_ORIGINS=https://digiurban.com.br,https://www.digiurban.com.br
CORS_CREDENTIALS=true

# Production Settings
NODE_ENV=production